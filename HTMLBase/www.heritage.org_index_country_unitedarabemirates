    var map = null;
    var geocoder = null;
    var globalZoom = 5;	

    function initialize(zoomLevel) {
	
        if (GBrowserIsCompatible()) {	
	    
            map = new GMap2(document.getElementById("map_canvas"));	        
            geocoder = new GClientGeocoder();
	        map.setMapType(G_PHYSICAL_MAP);
	        globalZoom = zoomLevel;
	       
        }
    }
    
    function geoResponse(point)
    {
        if (!point) {
            //Country not found
        }
        else {
            setMap(point.lat(), point.lng());
        }    
    }

    function showAddress(address) 
    {          
	    if (geocoder) {
            geocoder.getLatLng(address,geoResponse);
        }
    }
    
    function showLatLong(latitude, longitude) 
    {        
        setMap(latitude, longitude);
    }
    
    function setMap(latitude, longitude) {
        
        map.setCenter(new GLatLng(latitude,longitude), globalZoom);    
    }    

</script>

